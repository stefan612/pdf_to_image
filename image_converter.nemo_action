[Nemo Action]
Name=Convert Image
Comment=Convert image to jpg, png, pdf, gif, or ico
Exec=gnome-terminal -- bash -c 'python3 ~/scriptsLS/fileconverterTools/image_converter.py "%F"; read -p "press key..."'
#Exec=gnome-terminal -- bash -c 'python3 ~/scriptsLS/fileconverterTools/image_converter.py "%F";'
Icon-Name=image
Selection=s
Extensions=png;jpg;jpeg;gif;bmp;tiff;ico